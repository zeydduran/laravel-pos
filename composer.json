{
	"name": "zeydduran/laravel-pos",
	"type": "package",
	"description": "Laravel 8 Pos Package",
	"keywords": ["laravel8 pos", "laravel8 sanal pos"],
	"homepage": "https://github.com/zeydduran/laravel-pos",
	"license": "MIT",
	"authors": [
		{
			"name": "Zeyd DURAN",
			"email": "zeyd.duran@gmail.com",
			"homepage": "https://github.com/zeydduran",
			"role": "Developer"
		}
	],
	"require": {
		"php": "^7.1.3 || ^8.0",
		"illuminate/config": "~5.7|~5.8|^6.0|^7.0|^8.0",
		"mews/pos": "^0.4.3"
	},
	"require-dev": {
		"orchestra/testbench": "~3.4.0|~3.5.0",
		"phpunit/phpunit": "5.*|^7.9"
	},
	"autoload": {
		"psr-4": {
			"Mews\\LaravelPos\\": "src/"
		},
		"files": [
			"src/helpers.php"
		]
	},
	"autoload-dev": {
		"classmap": [
			"tests/TestCase.php"
		],
		"psr-4": {
			"Mews\\Test\\": "tests/"
		}
	},
	"minimum-stability": "stable"
}
